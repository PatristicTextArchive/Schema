               <xs:documentation>(analysis) for analysis of layout dependend structures in commentaries
Suggested values include: 1] marginal; 2] intercolumn; 3] interlinear; 4] scholia; 5] hexaplaric</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction>
                  <xs:simpleType>
                     <xs:list>
                        <xs:simpleType>
                           <xs:union>
                              <xs:simpleType>
                                 <xs:restriction base="xs:token">
                                    <xs:enumeration value="marginal">
                                       <xs:annotation>
                                          <xs:documentation>marginal commentary</xs:documentation>
                                       </xs:annotation>
                                    </xs:enumeration>
                                 </xs:restriction>
                              </xs:simpleType>
                              <xs:simpleType>
                                 <xs:restriction base="xs:token">
                                    <xs:enumeration value="intercolumn">
                                       <xs:annotation>
                                          <xs:documentation>Commentary between column of commented text and other commentary</xs:documentation>
                                       </xs:annotation>
                                    </xs:enumeration>
                                 </xs:restriction>
                              </xs:simpleType>
                              <xs:simpleType>
                                 <xs:restriction base="xs:token">
                                    <xs:enumeration value="interlinear">
                                       <xs:annotation>
                                          <xs:documentation>interlinear commentary</xs:documentation>
                                       </xs:annotation>
                                    </xs:enumeration>
                                 </xs:restriction>
                              </xs:simpleType>
                              <xs:simpleType>
                                 <xs:restriction base="xs:token">
                                    <xs:enumeration value="scholia">
                                       <xs:annotation>
                                          <xs:documentation>Scholia</xs:documentation>
                                       </xs:annotation>
                                    </xs:enumeration>
                                 </xs:restriction>
                              </xs:simpleType>
                              <xs:simpleType>
                                 <xs:restriction base="xs:token">
                                    <xs:enumeration value="hexaplaric">
                                       <xs:annotation>
                                          <xs:documentation>Hexaplaric readings</xs:documentation>
                                       </xs:annotation>
                                    </xs:enumeration>
                                 </xs:restriction>
                              </xs:simpleType>
                              <xs:simpleType>
                                 <xs:restriction base="xs:anyURI">
                                    <xs:pattern value="\S+"/>
                                 </xs:restriction>
                              </xs:simpleType>
                           </xs:union>
                        </xs:simpleType>
                     </xs:list>
                  </xs:simpleType>
                  <xs:minLength value="1"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
         <xs:attribute name="type" use="required">
            <xs:annotation>
               <xs:documentation/>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:token">
                  <xs:enumeration value="commentary">
                     <xs:annotation>
                        <xs:documentation>to contain the commentary to the edition</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="transcription">
                     <xs:annotation>
                        <xs:documentation>to contain the transcription of a text in a manuscript</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="edition">
                     <xs:annotation>
                        <xs:documentation>to contain the text of the edition itself; may include multiple text-parts</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="translation">
                     <xs:annotation>
                        <xs:documentation>to contain a translation of the text into one or more modern languages</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="praefatio">
                     <xs:annotation>
                        <xs:documentation>to contain the Praefatio to an edition</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="section">
                     <xs:annotation>
                        <xs:documentation>used to divide a div[type=praefatio] into multiple parts</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="subsection">
                     <xs:annotation>
                        <xs:documentation>used to divide a div[type=praefatio]/div[type=section] into multiple parts</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="textpart">
                     <xs:annotation>
                        <xs:documentation>used to divide a div[type=edition] into multiple parts (fragments, columns, faces, etc.)</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
         <xs:attribute name="subtype">
            <xs:annotation>
               <xs:documentation>(subtype) for the description of the text structure</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:token">
                  <xs:enumeration value="part">
                     <xs:annotation>
                        <xs:documentation>Part</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="book">
                     <xs:annotation>
                        <xs:documentation>Book</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="homily">
                     <xs:annotation>
                        <xs:documentation>Homily</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="chapter">
                     <xs:annotation>
                        <xs:documentation>Chapter</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="verse">
                     <xs:annotation>
                        <xs:documentation>Verse</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="subdok">
                     <xs:annotation>
                        <xs:documentation>Document belonging to a document of higher-order</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="praefatio">
                     <xs:annotation>
                        <xs:documentation>Preface to a book (n="praefatio")</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="section">
                     <xs:annotation>
                        <xs:documentation>Section</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="subsection">
                     <xs:annotation>
                        <xs:documentation>Subsection</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="fragment">
                     <xs:annotation>
                        <xs:documentation>Fragment</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="testimonium">
                     <xs:annotation>
                        <xs:documentation>Testimonium</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="commented">
                     <xs:annotation>
                        <xs:documentation>Biblical verses or text, which is commented on (comment in @subtype=commentary or scholia)</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="commentary">
                     <xs:annotation>
                        <xs:documentation>Exegesis of a Biblical verse or a commentary to a text</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="tei_model.teiHeaderPart" abstract="true"/>
   <xs:element name="tei_model.encodingDescPart" abstract="true"/>
